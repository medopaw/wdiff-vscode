    "version": "0.0.2",
    "publisher": "Med0paW",
    "icon": "images/logo.png",
    "license": "MIT",
    "author": "Med0paW <medopaw@gmail.com>",
    "repository": {
      "type": "git",
      "url": "https://github.com/medopaw/wdiff-vscode"
    },
